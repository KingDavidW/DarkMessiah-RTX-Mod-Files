"VertexLitGeneric"
{
	
	"$baseTexture"	"FX/charm/ch_Pattern_"
	
	"$SEPARATEALPHATEXTURE" "FX/charm/ch_Pattern_"



	"$additive" 1

	"$vertexcolor" 1
	"$vertexalpha" 1

	

	"$texscale" 6

	"$translucent" 1	

	"Proxies"
	{

	"TextureTransform"
		{
			"scaleVar"	"$texscale"
			
			"resultVar"	"$basetexturetransform"
		}

	"AnimatedTexture"
		{
			"animatedtexturevar" "$baseTexture"	
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 25.00
		}

	"AnimatedTexture"
		{
			"animatedtexturevar" "$SEPARATEALPHATEXTURE"
			"animatedtextureframenumvar" "$separatealphaframe"
			"animatedtextureframerate" 25.00
		}



	}
}
